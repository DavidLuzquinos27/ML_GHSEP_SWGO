ort pandas as pd

# Cargado de archivos
df1 = pd.read_parquet("ML_gamma_v2.parquet")
df2 = pd.read_parquet("ML_proton_v2.parquet")

# Concatenado de DataFrames
df_concat = pd.concat([df1, df2], ignore_index=True)

# Guardado en un nuevo parquet (opcional)
df_concat.to_csv("ML_aerie_gamma_proton_v4.csv", index=False,sep=',')

print("Completado - Total de registros:", len(df_concat))
